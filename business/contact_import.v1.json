{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Contact Import",
  "description": "Schema for importing contact information into based business management system. This schema is compatible with extraction/contact_details.json",
  "type": "object",
  "properties": {
    "name": {
      "type": "object",
      "properties": {
        "full_name": {
          "type": "string",
          "description": "Complete name"
        },
        "first_name": {
          "type": "string",
          "description": "First name"
        },
        "middle_name": {
          "type": "string",
          "description": "Middle name or initial"
        },
        "last_name": {
          "type": "string",
          "description": "Last name"
        },
        "prefix": {
          "type": "string",
          "description": "Title prefix (e.g., Dr., Mr., Ms.)"
        },
        "suffix": {
          "type": "string",
          "description": "Name suffix (e.g., Jr., III, PhD)"
        }
      },
      "required": [
        "full_name"
      ]
    },
    "job_title": {
      "type": "string",
      "description": "Professional title or position"
    },
    "company": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Company or organization name"
        },
        "department": {
          "type": "string",
          "description": "Department or division"
        },
        "industry": {
          "type": "string",
          "description": "Industry or sector"
        }
      }
    },
    "email_addresses": {
      "type": "array",
      "description": "Email addresses",
      "items": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "description": "Email address"
          },
          "type": {
            "type": "string",
            "enum": [
              "work",
              "personal",
              "other"
            ],
            "description": "Email type"
          },
          "primary": {
            "type": "boolean",
            "description": "Whether this is the primary email",
            "default": false
          }
        },
        "required": [
          "email"
        ]
      }
    },
    "phone_numbers": {
      "type": "array",
      "description": "Phone numbers",
      "items": {
        "type": "object",
        "properties": {
          "number": {
            "type": "string",
            "description": "Phone number"
          },
          "type": {
            "type": "string",
            "enum": [
              "work",
              "mobile",
              "home",
              "fax",
              "other"
            ],
            "description": "Phone type"
          },
          "extension": {
            "type": "string",
            "description": "Extension number if applicable"
          },
          "primary": {
            "type": "boolean",
            "description": "Whether this is the primary number",
            "default": false
          }
        },
        "required": [
          "number"
        ]
      }
    },
    "addresses": {
      "type": "array",
      "description": "Physical addresses",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "work",
              "home",
              "mailing",
              "other"
            ],
            "description": "Address type"
          },
          "street": {
            "type": "string",
            "description": "Street address"
          },
          "city": {
            "type": "string",
            "description": "City"
          },
          "state": {
            "type": "string",
            "description": "State or province"
          },
          "postal_code": {
            "type": "string",
            "description": "ZIP or postal code"
          },
          "country": {
            "type": "string",
            "description": "Country"
          },
          "full_address": {
            "type": "string",
            "description": "Complete formatted address"
          }
        }
      }
    },
    "websites": {
      "type": "array",
      "description": "Websites and online profiles",
      "items": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "Website URL"
          },
          "type": {
            "type": "string",
            "enum": [
              "company",
              "personal",
              "portfolio",
              "blog",
              "other"
            ],
            "description": "Website type"
          }
        },
        "required": [
          "url"
        ]
      }
    },
    "social_media": {
      "type": "object",
      "properties": {
        "linkedin": {
          "type": "string",
          "description": "LinkedIn profile URL or username"
        },
        "twitter": {
          "type": "string",
          "description": "Twitter/X handle"
        },
        "github": {
          "type": "string",
          "description": "GitHub username or profile URL"
        },
        "facebook": {
          "type": "string",
          "description": "Facebook profile"
        },
        "instagram": {
          "type": "string",
          "description": "Instagram handle"
        }
      }
    },
    "notes": {
      "type": "string",
      "description": "Additional notes or context about this contact"
    }
  },
  "required": [
    "name"
  ],
  "$id": "https://raw.githubusercontent.com/byteowlz/schemas/refs/heads/main/business/contact_import.v1.json",
  "schema_version": "1.0.0"
}
